#!/usr/bin/env php
<?php

require __DIR__ . '/../wp-load.php';

$application = new \Symfony\Component\Console\Application();
$application->addCommands([
    new Simply\Maker\Command\MakeCommand(),
    new Simply\Maker\Command\MakePostType(),
    new Simply\Maker\Command\MakeHook(),
    new Simply\Maker\Command\ClearCacheCommand()
]);
$application->run();
